.title {
  margin: 35rpx;
  border:  2px solid grey;
  border-top-style: none;
  border-left-style: none;
  border-right-style: none;
  padding: 20rpx;
  font-size: 14px;
}

.detail{
  margin: 45rpx;
  padding: 15rpx;
